#include <iostream>
#include <fstream>
#include <cstring>
using namespace std;


//hw scores and the class participation score, maximum 50.
float compute_hw_participation(istream& infile)
{
float hw, part, hwp; float total=0;
cout << " Scores are ";
for(int i=1; i <= 11; i++)
{
infile >> hw; 
infile >> part;
hwp = hw + part;
cout << hwp << endl;
}
return total;
}


public Class:myClass, class2, class3 {
}

